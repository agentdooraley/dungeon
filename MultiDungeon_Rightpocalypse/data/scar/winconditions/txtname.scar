function TxtName()
        local txtname = ""
	local num_player = World_GetPlayerCount() 	

	for i = 0, num_player - 1 do
          local playerID = World_GetPlayerAt( i )
          local playerRace = Player_GetRaceName(playerID)
          local bufer = 0
	  if (player_IsHuman_T(playerID) == false) then
          local numtab = table.getn(supername)
          bufer = World_GetRand( 1, numtab )
           txtname = supername[bufer]
	      Setup_SetPlayerName(playerID, txtname)
          end
	end	

end

Scar_AddInit(TxtName)

function player_IsHuman_T( player )
	local name = Player_GetDisplayName(player)[1]
	if string.len(name) < 3 then return true end
	name = string.sub(name, -2)
	if( name == " 1" or name == " 2" or name == " 3" or name == " 4" or name == " 5" or name == " 6" or name == " 7" or name == " 8") then
		return false
	else
		return true
	end
end