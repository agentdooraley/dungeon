function Rule_CheckPlayersTech()
	for i=0, g_number_of_players-2 do
		--Srvl_PlayerInfo.Techlvl[i] = 1
		local level_found = false
	-- Search for Tech 4 ----
		if Srvl_PlayerInfo.Tech4Info[i][1] == "RSRCH" then
			for j=2, table.getn(Srvl_PlayerInfo.Tech4Info[i]) do
				if Player_GetResearchState(Srvl_PlayerInfo.PlayerID[i],Srvl_PlayerInfo.Tech4Info[i][j])==RS_Complete then
					Srvl_PlayerInfo.Techlvl[i] = 4; level_found = true
				end
			end
		elseif Srvl_PlayerInfo.Tech4Info[i][1] == "BLDNG" then
			for j=2, table.getn(Srvl_PlayerInfo.Tech4Info[i]) do
				if Player_HasBuildingType(Srvl_PlayerInfo.PlayerID[i],Srvl_PlayerInfo.Tech4Info[i][j]) then
					Srvl_PlayerInfo.Techlvl[i] = 4; level_found = true
				end
			end
		elseif Srvl_PlayerInfo.Tech4Info[i][1] == "ADDON" then
			local surv_temp_egroupID_player = Player_GetEntities(Srvl_PlayerInfo.PlayerID[i])
			for j=2, table.getn(Srvl_PlayerInfo.Tech4Info[i]) do
				if EGroup_ContainsAddOn(surv_temp_egroupID_player,Srvl_PlayerInfo.Tech4Info[i][j],false) then
					Srvl_PlayerInfo.Techlvl[i] = 4; level_found = true
				end
			end
		else print("Problem in calculating race "..Srvl_PlayerInfo.RaceName[i].." Tech 4 level!")
		end
if Global_RaceStats[Srvl_PlayerInfo.RaceName[i]].Tier4A ~= nil then
		if Srvl_PlayerInfo.TechA4Info[i][1] == "RSRCH" then
