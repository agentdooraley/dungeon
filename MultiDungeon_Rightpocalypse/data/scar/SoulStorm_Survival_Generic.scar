	--if G_SlagFestSwitchEnabled == true then
		local dummyEntity = Entity_Create( "dummy_entity", World_GetPlayerAt( 0 ), World_Pos( 0, 0, 0 ) )
		local maxWorldId = dummyEntity.id - 1
		Entity_Destroy( dummyEntity )
		local tempEntity = false
		for i = 0, maxWorldId do
			tempEntity = Rule_Slag_EntityExists( i )
				if tempEntity
				and Entity_GetBlueprintName( tempEntity ) == "slag_deposit" then
				--print "found a slag"
				slag_location = Entity_GetPosition(tempEntity )
				SGroup_CreateIfNotFound("raiders_of_the_lost_slag"..i)
				SGroup_Clear("raiders_of_the_lost_slag"..i)
				-- Is this enough?
				Util_CreateSquadsAtMarkerEx(g_enemy_PlayerID, "raiders_of_the_lost_slag"..i, DeCapturingSquadName, "mk_enemy_pos", 5, DeCapturingSquadMmbrs)
				SGroup_AddLeaders("raiders_of_the_lost_slag"..i)
				Rule_LockSGroupSurvivalPlayer("raiders_of_the_lost_slag"..i)
				Cmd_AttackMovePos("raiders_of_the_lost_slag"..i, slag_location)
				return true
				end

		end
		return false
	--end
	
end


