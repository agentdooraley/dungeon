function Rule_Player_Rusher()
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			temp_surv_sgID = SGroup_CreateIfNotFound("sg_Player_squads_surv_temp")
			SGroup_Clear(temp_surv_sgID)
			SGroup_AddGroup(temp_surv_sgID, Player_GetSquads(Srvl_PlayerInfo.PlayerID[i]))
			if Prox_AnySquadNearMarker("sg_Player_squads_surv_temp","mk_enemy_pos") then
			Rule_Remove(Rule_Doomwave_Safety_Check)
				EventCue_DoEvent ("warning", "/event_cue_notifications/team_change", "Doom Wave Triggered!", "We got too near the enemy HQ, they are sending everything they've got at us!")
				print("Deterrent against player "..(i+1).." !!!")
				Rule_DefensiveDeterrentPlayer()
				Rule_LockSGroupSurvivalPlayer("sg_deterrent_one")
				Cmd_AttackMovePos("sg_deterrent_one",Srvl_PlayerInfo.HQpos[i])
				Rule_AddInterval(Rule_Reinstate_Deterrent,4)
				Rule_Remove(Rule_Player_Rusher)

				return
			end
		end
	end
end


function Rule_Reinstate_Deterrent()
	if SGroup_CountSpawned(SGroup_FromName("sg_deterrent_one"))==0 then
		SGroup_Destroy(SGroup_FromName("sg_deterrent_one"))
		Util_MissionTitle("The Enemy's strongest warriors have fallen - consign the foe to history before they can regroup!")
		--fuggles - this makes it loop again
		--Rule_AddIntervalDelay(Rule_Player_Rusher,5.125,30)
		Rule_Remove(Rule_Reinstate_Deterrent)
			if ranking_condition_toggle == true then
				if g_fuggles_rank_flag == 0 then
				--print "increase p1 rank, should run once?"	
				Rule_AddOneShot(Rule_IncreasePlayerOneRank,2)
				g_fuggles_rank_flag = 1
				end
			end
	end
end


