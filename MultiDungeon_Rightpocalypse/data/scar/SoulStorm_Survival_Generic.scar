function Rule_DifficultySetByFirstHumanPlayer()
	g_Surv_DifficultyTimer = World_GetGameTime()
	-- Spawn difficulty building for first Human player.
	for idx=0, g_number_of_players-1 do
		local pID = World_GetPlayerAt(idx)
		if Player_IsHuman(pID) then
			g_Survival_playerID_Difficulty_Choose = pID
			break
		end
	end
	local pos = Player_GetStartPosition(g_Survival_playerID_Difficulty_Choose); pos.x = pos.x - 13
	Surv_DifficultySpawnEntity = Entity_Create("surv_diff_building_dummy", g_Survival_playerID_Difficulty_Choose, pos)
	Entity_Spawn(Surv_DifficultySpawnEntity)
	local eg_Surv_diff_dummyID = EGroup_CreateIfNotFound( "eg_Surv_diff_dummy_group0" )
	EGroup_Add("eg_Surv_diff_dummy_group0", Surv_DifficultySpawnEntity)
	if Tut_GetLocalPlayerIndex() == World_GetPlayerIndex(g_Survival_playerID_Difficulty_Choose) then
		W40k_SelectEGroup(eg_Surv_diff_dummyID)
		Camera_FocusOnTargetPos(pos, 1)
	end
	-- Start checking which difficulty hew chose
	Rule_Add(Rule_DifficultyChosenByFirstHumanPlayer)
	Rule_Add(Rule_Display_MP_Diff_Choice_SRVVL)
end


function Rule_DifficultyChosenByFirstHumanPlayer()
	local dif = ""
	local checkDiff = function( sgroupid, itemindex, squadID )
		local name = Squad_GetBlueprintName( squadID )
		if name == "surv_squad_diff_easy_dummy" then dif = "easy"; Squad_Destroy(squadID); return true
		elseif name == "surv_squad_diff_normal_dummy" then dif = "standard"; Squad_Destroy(squadID); return true
		elseif name == "surv_squad_diff_hard_dummy" then dif = "hard"; Squad_Destroy(squadID); return true
		elseif name == "surv_squad_diff_harder_dummy" then dif = "harder"; Squad_Destroy(squadID); return true
		elseif name == "surv_squad_diff_insane_dummy" then dif = "insane"; Squad_Destroy(squadID); return true end
	end
	if SGroup_ForEachAllOrAnyEx( Player_GetSquads(g_Survival_playerID_Difficulty_Choose), false, checkDiff, true, true ) then
		Entity_DeSpawn(Surv_DifficultySpawnEntity)
		Entity_Destroy(Surv_DifficultySpawnEntity)
		g_difficulty = dif
		Rule_Remove(Rule_DifficultyChosenByFirstHumanPlayer)
		Rule_AddOneShot(Rule_DifficultyModificationHPs,0.5)
		Rule_AddInterval(Rule_Display_Credits,6)
		print("MP Final Difficulty: "..g_difficulty)
		local squadID = SGroup_GetSpawnedSquadAt(Player_GetSquads(g_Survival_playerID_Difficulty_Choose),1)
		if squadID ~= nil then
			local sg_Surv_diff_dummyID = SGroup_CreateIfNotFound( "sg_Surv_diff_dummy_group0" )
			SGroup_Add("sg_Surv_diff_dummy_group0", squadID )
			if Tut_GetLocalPlayerIndex() == World_GetPlayerIndex(g_Survival_playerID_Difficulty_Choose) then
				W40k_SelectSGroup(sg_Surv_diff_dummyID)
				Camera_FocusOnTargetPos(SGroup_GetPosition("sg_Surv_diff_dummy_group0"), 0)
			end
		end	
	elseif World_GetGameTime() > g_Surv_DifficultyTimer + 12 then
		-- Time is up, randomly choose difficulty
		local random = World_GetRand(1,20)
		if random < 4 then g_difficulty = "easy"
		elseif random < 7 then g_difficulty = "standard"
		elseif random < 10 then g_difficulty = "hard"
		elseif random < 15 then g_difficulty = "harder"
		else g_difficulty = "insane" end
		Entity_DeSpawn(Surv_DifficultySpawnEntity)
		Entity_Destroy(Surv_DifficultySpawnEntity)
		Rule_Remove(Rule_DifficultyChosenByFirstHumanPlayer)
		Rule_AddOneShot(Rule_DifficultyModificationHPs,0.5)
		Rule_AddInterval(Rule_Display_Credits,6)
		print("MP Final Difficulty: "..g_difficulty)
	end
end


