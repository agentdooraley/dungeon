function Rule_UpdatePlayerLevels()
	local p_level = "/ "
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			if Srvl_PlayerInfo.Level[i] > Surv_Max_Waves then
				p_level = p_level..Srvl_PlayerInfo.PlayerName[i].." - Free to attack / "
			else
				p_level = p_level..Srvl_PlayerInfo.PlayerName[i].." - Wave "..Srvl_PlayerInfo.Level[i].." / 10 "
			end
		else
			p_level = p_level..Srvl_PlayerInfo.PlayerName[i].." - Lvl X / "
		end
	end
	WinWarning_SetText( "Player_level_surv", p_level )
end


function Rule_StartAttackingWaves()
	-- Update levelling text
	Rule_UpdatePlayerLevels()
	-- Check for waves and players
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			local sg = "sg_level"..Srvl_PlayerInfo.Level[i].."_P"..i
			if not SGroup_Exists(sg) then
				SGroup_CreateIfNotFound(sg)
				-- These Variables set the time and condition of the next wave for this player
				Srvl_PlayerInfo.TMR[i] = World_GetGameTime() + g_wavetimer
				Srvl_PlayerInfo.LvlSwitch[i] = Srvl_PlayerInfo.Level[i]
				if Tut_GetLocalPlayerIndex() == i then
					if Srvl_PlayerInfo.Level[i] == 1 then
						Rule_Add(Rule_Display_First_Attack_SRVVL)
						if g_Survival_Enemy_AI_Is_Human then
							Rule_AddDelay(Rule_Display_Enemy_Human_Player_SRVVL, 10)
						end
					else
						EventCue_DoEvent ("warning", "/event_cue_notifications/team_change", "Level Increased to "..Srvl_PlayerInfo.Level[i].."!","The enemy has been defeated, but only temporarily. This defeat has strengthen his resolve - he has advanced to the next assault level!")
               --"survival", surely?
					end
				end
          --fuggles -> timer here?
          --can I just remove the and?
			--elseif World_GetGameTime() > Srvl_PlayerInfo.TMR[i] + 6 and SGroup_CountSpawned(sg) == 0 then
			elseif World_GetGameTime() > Srvl_PlayerInfo.TMR[i] + (Srvl_PlayerInfo.Level[i] * 30) + 120 then
				SGroup_Destroy(sg)
				--fuggles

			 	
				if G_ShortDefenceSwitchEnabled ~= nil then
					--print "skipping a level"
					levelchange = 2
				else
					--print "not skipping a level"
					levelchange = 1
				end
				-- I just need it to end on 10, I feel uncomfortable otherwise... I got issues.
				if Srvl_PlayerInfo.Level[i] == 1 then
					levelchange = 1
				end
				Srvl_PlayerInfo.Level[i] = Srvl_PlayerInfo.Level[i] + levelchange

			elseif World_GetGameTime() > Srvl_PlayerInfo.TMR[i] + 6 and SGroup_CountSpawned(sg) == 0 then
				SGroup_Destroy(sg)
		 	
				if G_ShortDefenceSwitchEnabled ~= nil then
					--print "skipping a level"
					levelchange = 2
				else
					--print "not skipping a level"
					levelchange = 1
				end
				-- I just need it to end on 10, I feel uncomfortable otherwise... I got issues.
				if Srvl_PlayerInfo.Level[i] == 1 then
					levelchange = 1
				end
				Srvl_PlayerInfo.Level[i] = Srvl_PlayerInfo.Level[i] + levelchange
			end
		end
	end
end

