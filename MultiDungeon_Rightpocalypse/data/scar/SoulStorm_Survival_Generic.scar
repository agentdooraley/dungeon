-- If waves fail, FULLY ENABLE the AI of the computer!
function Rule_Attack_SetThingsAtEnd()
	-- First, REMOVE the previous Difficulty settings rule, that was responsible for AI Difficulty
	--  in case we have an attacking AI, and not an attacking Player
	if not g_Survival_Enemy_AI_Is_Human then
		print("Removing AI rules for SP game...")
		Rule_Check_Remove(Rule_CPUPlayer_AI_Difficulty2)
		Rule_Check_Remove(Rule_CPUPlayer_AI_Difficulty3)
		Rule_Check_Remove(Rule_CPUPlayer_AI_Difficulty4)
		Rule_Check_Remove(Rule_CPUPlayer_AI_Difficulty5)		
		Cpu_UnlockAll( g_enemy_PlayerID )
	end
	print("Waves failed, now the enemy will defend/attack as if in a standard game!")
	local alive_players = 0
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			alive_players = alive_players + 1
		end
	end
	-- For TWO alive attackers Vs AI, always set Difficulty to Insane after last wave
	-- DOES NOT WORK. Cpu_SetDifficulty (if different than original) causes recourse desyncing...
	--[[if alive_players > 1 then
		Cpu_SetDifficulty(g_enemy_PlayerID, AD_Insane)  -- (AD_Easy, AD_Standard, AD_Hard, AD_Advanced, AD_Insane)
		if not Player_IsHuman(g_enemy_PlayerID) then
			print("More than one player alive - setting AI difficulty for local (controlling) player to Insane")
			pcall(Cpu_Enable,g_enemy_PlayerID, false)
		end
	end]]
	-- For TWO+ alive attackers Vs AI in Insane, increase caps for CPU player...
	if alive_players > 1 and g_difficulty == "insane" then
		print("More than one player alive - setting max Enemy AI caps x 1.5")
		local oSurvMaxSquadCap = Modifier_Create(MAT_Player, "max_squad_cap_player_modifier", MUT_Multiplication, false, 1.5, "")
		local oSurvMaxSuprtCap = Modifier_Create(MAT_Player, "max_support_cap_player_modifier", MUT_Multiplication, false, 1.5, "")
		local oSurvSquadCap = Modifier_Create(MAT_Player, "squad_cap_player_modifier", MUT_Multiplication, false, 1.5, "")
		local oSurvSuprtCap = Modifier_Create(MAT_Player, "support_cap_player_modifier", MUT_Multiplication, false, 1.5, "")
		-- Apply army cap modifiers
		pcall(Modifier_ApplyToPlayer, oSurvMaxSquadCap, g_enemy_PlayerID)
		pcall(Modifier_ApplyToPlayer, oSurvMaxSuprtCap, g_enemy_PlayerID)
		pcall(Modifier_ApplyToPlayer, oSurvSquadCap, g_enemy_PlayerID)
		pcall(Modifier_ApplyToPlayer, oSurvSuprtCap, g_enemy_PlayerID)
	end
	-- Completely re-initiate the AI
	if not Player_IsHuman(g_enemy_PlayerID) then
		Cpu_Enable(g_enemy_PlayerID, true)
	end
	-- Re-enable any possible AI ally attacking tactics
	Rule_Remove(Rule_CPUAllyPlayer_AI_Difficulty)
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) and not Player_IsHuman(Srvl_PlayerInfo.PlayerID[i]) then
			print("Re-enabling attacking tactics for Ally "..i)
			Cpu_EnableComponent(Srvl_PlayerInfo.PlayerID[i], true, CT_Attacking)
		end
	end
end

