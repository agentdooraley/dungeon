function Rule_RewardComp()
	if g_Survival_Enemy_AI_Is_Human then return end
	local factor = 0.5
	if g_difficulty == "hard" or g_difficulty == "harder" then factor = 1
	elseif g_difficulty=="insane" then factor = 1.5 end
	local req = (g_number_of_players-1) * factor * 50
	local pow = (g_number_of_players-1) * factor * 25
	Player_AddResource(g_enemy_PlayerID,RT_Requisition,req)
	Player_AddResource(g_enemy_PlayerID,RT_Power,pow)
end



--[[TRIGGER THE MAIN RULES TO SET THINGS GOING]]--
function Rule_KickThingsOff()
	if not (g_Survival_Enemy_AI_Is_Human or g_difficulty == "insane" or g_difficulty == "harder") then
		Player_SetResource(g_enemy_PlayerID,RT_Requisition,0)
		Player_SetResource(g_enemy_PlayerID,RT_Power,0)
	end
	-- Random spamming squad attacks rule
	Rule_AddInterval(Rule_HarassingSquadsBegin,g_spamtimer)
--aircraft tuning 1, g_spamtimer on harder is about 2.5 minutes
	if air_attack_toggle == true then
	Rule_AddIntervalDelay(Rule_HarassingAirSquadsBegin,g_spamtimer*2,600)
	else
	print "air support not authorised"
	end
	-- Waves rule
	Rule_AddIntervalDelay(Rule_StartAttackingWaves,2.5,g_wavetimer+0.125)
	Rule_AddIntervalDelay(Rule_SpawnAttackingWaves,2.5,g_wavetimer+1.25)
	-- Deterrent rule
	Rule_AddInterval(Rule_Player_Rusher,5.125)
	-- Add Toggle?
	Rule_AddInterval(Rule_Doomwave_Safety_Check,2.5)
	-- Tech Rule
	Rule_AddIntervalDelay(Rule_CheckPlayersTech,1,0.25)
	-- HQ detection rule
	Rule_AddIntervalDelay(Rule_DetectPlayersHQLocations,2,0.5)
	print("Rules added!")
	-- AI Difficulty rules
	if Cpu_IsCpuPlayer(g_enemy_PlayerID) then
		print("Local PC, initially CONTROLS the attacking AI!!!")
	else
		print("Local PC, initially does NOT control the attacking AI.")
	end
	-- If against AI, set attacking AI (ONLY) difficulty, based on difficulty chosen
	if not g_Survival_Enemy_AI_Is_Human then
	-- Because in single player games you can save, the rule must be repeated
	-- Plus, the difficulty here can be determined MORE accurately, so we have more complex AI settings.
		if g_difficulty=="easy" or g_difficulty == "standard" then Rule_AddOneShot(Rule_CPUPlayer_AI_Difficulty1,5)
		elseif g_difficulty == "hard" then Rule_AddOneShot(Rule_CPUPlayer_AI_Difficulty2,5)
		elseif g_difficulty=="harder" then Rule_AddOneShot(Rule_CPUPlayer_AI_Difficulty3,5)
		elseif g_difficulty=="insane" then Rule_AddOneShot(Rule_CPUPlayer_AI_Difficulty4,5) end
	end
	-- Disable any possible AI allies from attacking. Keep it on always, in case the Attacking player is Human!
	-- if g_number_of_players - g_number_of_human_defending_players > 1 then
		print("CPU Defending Allies detected. Disabling their attacking tactics...")
		Rule_AddInterval(Rule_CPUAllyPlayer_AI_Difficulty,0.75)
	-- end
	Rule_AddOneShot(Rule_SetDifficultyTimer,4)
	Rule_AddOneShot(Rule_BodyCountAndLevel_Triggers,6)
	Rule_AddIntervalDelay(Rule_RewardComp,30,13.5)

	if ranking_condition_toggle == true then
	print "ranked match"
	fuggles_rank_flag = 1
	Rule_AddOneShot(Rule_GetPlayersCurrentRankP1,3)
	else
	print "non ranked match"
	end
end


