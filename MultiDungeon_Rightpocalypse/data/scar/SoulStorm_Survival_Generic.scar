function Rule_SpawnAttackingWaves()
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			if Srvl_PlayerInfo.Level[i] > Surv_Max_Waves then
			-- The player has defeated the LAST wave!
				if Tut_GetLocalPlayerIndex() == World_GetPlayerIndex(Srvl_PlayerInfo.PlayerID[i]) then
					EventCue_DoEvent ("warning", "/event_cue_notifications/team_change", "Enemy Assault Failed","The enemy has exhausted all his resources and reinforcements and can no longer assault your base. Now is the time to finish him once an for all!")
				end	
					if ranking_condition_toggle == true then
						if g_fuggles_rank_flag == 0 then
							--print "increase p1 rank, should run once?"	
							Rule_AddOneShot(Rule_IncreasePlayerOneRank,2)
						g_fuggles_rank_flag = 1
						end
					end
				-- Global var set to true, so that other win conditions to know that waves failed.
				g_Survival_Enemy_Waves_Have_Failed = true
				Rule_UpdatePlayerLevels()
				if Rule_Exists(Rule_Player_Rusher) then Rule_Remove(Rule_Player_Rusher) end
				if Rule_Exists(Rule_Doomwave_Safety_Check) then Rule_Remove(Rule_Doomwave_Safety_Check) end
				if Rule_Exists(Rule_Reinstate_Deterrent) then Rule_Remove(Rule_Reinstate_Deterrent) end
				--Rule_Remove(Rule_BodyCount_Players) -- let's KEEP counting bodies afterwards!!
				Rule_Remove(Rule_HarassingSquadsBegin)
				Rule_Remove(Rule_StartAttackingWaves)
				--Rule_Remove(Rule_CheckPlayersTech) -- let's KEEP checking for Tech, in case we use CL Reinforcements!!
				Rule_Remove(Rule_DetectPlayersHQLocations)
				Rule_Remove(Rule_RewardComp)
				Rule_Remove(Rule_CaptureCrits)
				Rule_Remove(Rule_CaptureRelics)
				Rule_Remove(Rule_Force_Attack_Idle_Squads)
				Rule_Remove(Rule_CheckStuckSquads)
				Rule_Remove(Rule_SpawnAttackingWaves)
				Rule_AddInterval(Rule_Display_SurvWinMessage,3)
				Rule_Attack_SetThingsAtEnd()
				-- Now, DESTROY ALL "wrongly" despawned squads, so that the enemy player to start with fresh caps
				-- In case some squads were not spawned properly!
				-- Regrettably, there IS support for Buildings, but NOT for vehicles that carry quads... :(
				-- It would be TOO tedious to code it, just for Survival. I did it only for the Race Aircraft (DA/UM) carriers.
				local destroyer_exceptions = Global_RaceStats[g_enemy_RaceName].Surv_Entrench_Squads
				local enemy_buildingsEG = Player_GetEntities(g_enemy_PlayerID)
				local non_spawned_destroyer = function( sgroupid, itemindex, squadID )
					if not Squad_IsInHold(squadID,enemy_buildingsEG) then
						Squad_Destroy(squadID)
					end
				end
				local non_spawned_destroyer_special = function( sgroupid, itemindex, squadID )
					local squad_name = Squad_GetBlueprintName(squadID)
					for j = 1, table.getn(destroyer_exceptions) do
						if squad_name == destroyer_exceptions[j] then
							print(squad_name.." NOT destroyed!")
							break
						end
						if not Squad_IsInHold(squadID,enemy_buildingsEG) then
							Squad_Destroy(squadID)
						end
					end
				end
				if destroyer_exceptions ~= nil then
					print("...we have exceptions in destroying despawned squads")
					SGroup_ForEachEx(Player_GetSquads(g_enemy_PlayerID),non_spawned_destroyer_special,false,true)
				else
					SGroup_ForEachEx(Player_GetSquads(g_enemy_PlayerID),non_spawned_destroyer,false,true)
				end
				Rule_UnlockSGroupSurvivalPlayer(Player_GetSquads(g_enemy_PlayerID))
				return
			elseif World_GetGameTime() > Srvl_PlayerInfo.TMR[i] and Srvl_PlayerInfo.LvlSwitch[i] == Srvl_PlayerInfo.Level[i] then
				g_SurvPlayerWaveSpawnTimeAndPosition[i].Time = World_GetGameTime()
				g_SurvPlayerWaveSpawnTimeAndPosition[i].Pos = nil
				g_SurvPlayerWaveSpawnTimeAndPosition[i].SGPos = nil
				Srvl_PlayerInfo.LvlSwitch[i] = - 1 -- Switch, for ONE delivery
				print("Attacking Player "..(i+1).." with wave "..Srvl_PlayerInfo.Level[i])
				--fuggles, this bit here
				Rule_WavesAttacks_Player(i,Srvl_PlayerInfo.Level[i])
				local sg = "sg_level"..Srvl_PlayerInfo.Level[i].."_P"..i
				Rule_LockSGroupSurvivalPlayer(sg)
				Cmd_AttackMovePos(sg,Srvl_PlayerInfo.HQpos[i])
				Rule_LockSGroupSurvivalPlayer(sg)
				Cmd_AttackMovePos(sg,Srvl_PlayerInfo.HQpos[i])			
					if g_SPMP == 1 then
					--print "Whiskey drinker detected"
					g_SPMP_trigger = 1			
					Rule_WavesAttacks_Player(i,Srvl_PlayerInfo.Level[i])
					local sg = "sg_level"..Srvl_PlayerInfo.Level[i].."_P"..i
					Rule_LockSGroupSurvivalPlayer(sg)
					Cmd_AttackMovePos(sg,Srvl_PlayerInfo.HQpos[i])
					Rule_LockSGroupSurvivalPlayer(sg)
					Cmd_AttackMovePos(sg,Srvl_PlayerInfo.HQpos[i])	
					g_SPMP_trigger = 0
					end
				
				return -- So than NOT to spawn the waves over the SAME timeframe
			end
		end
	end
end


