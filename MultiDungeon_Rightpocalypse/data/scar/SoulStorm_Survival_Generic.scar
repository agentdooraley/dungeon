end


function Rule_Force_Attack_Idle_Squads()
	-- If the attacking player is actually Human, then HE controls idle squads!
	if g_Survival_Enemy_AI_Is_Human then return end
	print("Force idle or misplaced squads to attack!")
	-- Create groups to "house" idle squads
	local max = g_number_of_players-2
	for i=0, max do
		SGroup_Clear(SGroup_CreateIfNotFound("sg_survival_ai_idle_squads"..i))
	end
	-- Find idle squads, and add them to a random group, so that to attach a random player!
	local idle_finder  = function( sgroupid, itemindex, squadID )
		local command = Squad_GetActiveCommand(squadID)
		if (command == 0 or command == 31) -- (31 is for Burrow)
		and Squad_GetBlueprintName(squadID) ~= Srvl_PlayerInfo.PriBuilderSqdName[g_number_of_players-1][1] then
			SGroup_Add("sg_survival_ai_idle_squads"..World_GetRand(0,max),squadID)
		end
	end
	-- First, get all player 5 squads
	local all_EnemyPlayer_eligible_SG = Player_GetSquads(g_enemy_PlayerID)
	-- Special: if we have the destroy_buildigs wincondition, DO NOT include any possible spawned squads!
	local building_possibly_excluded_SG = SGroup_CreateIfNotFound("sg_defenders_db_total_survival"); SGroup_Clear(building_possibly_excluded_SG)
	if SGroup_Exists("sg_defenders_db_1") then SGroup_AddGroup(building_possibly_excluded_SG, SGroup_FromName("sg_defenders_db_1")) end
	if SGroup_Exists("sg_defenders_db_2") then SGroup_AddGroup(building_possibly_excluded_SG, SGroup_FromName("sg_defenders_db_2")) end
	if SGroup_Exists("sg_defenders_db_3") then SGroup_AddGroup(building_possibly_excluded_SG, SGroup_FromName("sg_defenders_db_3")) end
	if SGroup_Exists("sg_defenders_db_4") then SGroup_AddGroup(building_possibly_excluded_SG, SGroup_FromName("sg_defenders_db_4")) end
	if SGroup_Exists("sg_defenders_db_5") then SGroup_AddGroup(building_possibly_excluded_SG, SGroup_FromName("sg_defenders_db_5")) end
	if SGroup_Exists("sg_defenders_db_6") then SGroup_AddGroup(building_possibly_excluded_SG, SGroup_FromName("sg_defenders_db_6")) end
	if SGroup_Count(building_possibly_excluded_SG) > 0 then Sgroup_RemoveSgroupSurvival(all_EnemyPlayer_eligible_SG, building_possibly_excluded_SG) end
	-- Now, perform the check for the really eligible squads
	SGroup_ForEachEx( all_EnemyPlayer_eligible_SG, idle_finder, true, false )
	-- Perform the attack, based on ALIVE players
	local alive_players = {}
	for i=0, max do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			table.insert(alive_players,i)
		end
	end
	local total_alive = table.getn(alive_players)
	for i=0, max do
		local sg = "sg_survival_ai_idle_squads"..i
		if SGroup_Count(sg) > 0 then
			Rule_LockSGroupSurvivalPlayer(sg)
			if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
				print("Sending "..SGroup_Count(sg).." idle squads, against Player "..(i+1))
				Cmd_AttackMovePos(sg,Srvl_PlayerInfo.HQpos[i])
			else
				rand_alive_player = alive_players[World_GetRand(1,total_alive)]
				print("Sending "..SGroup_Count(sg).." idle squads, against Player "..(rand_alive_player+1))
				Cmd_AttackMovePos(sg,Srvl_PlayerInfo.HQpos[rand_alive_player])
			end
		end
	end
end


function Rule_CheckStuckSquads()
	local time = World_GetGameTime()
	for i=0, g_number_of_players-2 do
		if Player_IsAlive(Srvl_PlayerInfo.PlayerID[i]) then
			local sgroupName = "sg_level"..Srvl_PlayerInfo.Level[i].."_P"..i
			if SGroup_Exists(sgroupName) and SGroup_CountSpawned(sgroupName) > 0 then
				if time > g_SurvPlayerWaveSpawnTimeAndPosition[i].Time + 900 then
				-- Force next wave after 15 minutes, regardless situation!
					print("Reached 15 minutes limit for player "..(i+1).." in wave "..Srvl_PlayerInfo.Level[i]..". Forcing next wave...")
					SGroup_Clear(sgroupName)
				elseif time > g_SurvPlayerWaveSpawnTimeAndPosition[i].Time + 400 then
				-- Setting time check to 400, makes the check after 7.5 + 90 mins (1st check) for a wave (the rule has a 90 interval)
					local sgroup_pos = SGroup_GetPosition(sgroupName)
					local remote_squad_pos = Squad_GetPosition(SGroup_GetSpawnedSquadAt(sgroupName,1))
					local attack_pos = Srvl_PlayerInfo.HQpos[i]
					local temp_pos = nil
					local most_remote_squad = function(sgroupid, itemindex, squadID)
						temp_pos = Squad_GetPosition(squadID)
						if World_DistancePointToPoint(temp_pos,attack_pos) > World_DistancePointToPoint(remote_squad_pos,attack_pos) then
							remote_squad_pos = temp_pos
						end
					end
					SGroup_ForEach(sgroupName, most_remote_squad)
					if g_SurvPlayerWaveSpawnTimeAndPosition[i].Pos ~= nil then
						if  World_DistancePointToPoint(remote_squad_pos,g_SurvPlayerWaveSpawnTimeAndPosition[i].Pos) < 10
						and World_DistancePointToPoint(sgroup_pos,g_SurvPlayerWaveSpawnTimeAndPosition[i].SGPos) < 10 then
							if not SGroup_IsUnderAttack(sgroupName,false) then
								print("Possible stuck squads for player "..(i+1).." in wave "..Srvl_PlayerInfo.Level[i]..". Forcing next wave...")
								SGroup_Clear(sgroupName)
							end
						end
					end
					g_SurvPlayerWaveSpawnTimeAndPosition[i].Pos = remote_squad_pos
					g_SurvPlayerWaveSpawnTimeAndPosition[i].SGPos = sgroup_pos
				end
			end
		end
	end
end


